{
  "name": "react-postprocessing",
  "version": "1.0.0-beta4",
  "description": "postprocessing wrapper for React and react-three-fiber",
  "keywords": [
    "postprocessing",
    "react",
    "three",
    "react-three-fiber",
    "webgl",
    "3d"
  ],
  "author": "v1rtl",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-spring/react-postprocessing.git"
  },
  "scripts": {
    "build:esm": "tsc",
    "build:cjs": "tsc -p cjs.tsconfig.json",
    "build": "yarn build:esm && pnpm run build:cjs",
    "prepare": "yarn build",
    "eslint": "eslint src/**/*.{js,ts,jsx,tsx}",
    "test": "echo no tests yet",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false --strict --jsx react"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(js|jsx|ts|tsx)'"
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 120
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "postprocessing": "^6.13.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-three-fiber": "^4.1.4",
    "three": "^0.116.1",
    "tsd-jsdoc": "^2.5.0"
  },
  "peerDependencies": {
    "postprocessing": "^6.13.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-three-fiber": "^4.1.4",
    "three": "^0.116.1"
  },
  "dependencies": {
    "react-merge-refs": "^1.0.0"
  }
}
